# integer
tipe_data = 10
print("data = ", tipe_data) # menampilkan nilai variabel pada terminal
print("tipe data = ", type(tipe_data)) #menampilkan tipe data yang digunakan

#  float
tipe_data = 1.7
print("data = ", tipe_data) # menampilkan nilai variabel pada terminal
print("tipe data = ", type(tipe_data)) #menampilkan tipe data yang digunakan

# string
tipe_data = "universitas negeri padang"
print("data = ", tipe_data) # menampilkan nilai variabel pada terminal
print("tipe data = ", type(tipe_data)) #menampilkan tipe data yang digunakan

# boolean
tipe_data = True
print("data = ", tipe_data) # menampilkan nilai variabel pada terminal
print("tipe data = ", type(tipe_data)) #menampilkan tipe data yang digunakan

# bilangan kompleks
tipe_data = complex(2,6)
print("data = ", tipe_data) # menampilkan nilai variabel pada terminal
print("tipe data = ", type(tipe_data)) #menampilkan tipe data yang digunakan
